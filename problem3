#include <bits/stdc++.h>
using namespace std;


int main() {
    int n;
    cin>>n;
    int a[n],b[n];
    int c=0;
    for(int i=0;i<n;i++)
    {
        int in;
        cin>>in;
        a[i]=in;
        b[i]=in;
    }
    sort(a,a+n);
    
    for(int i=0;i<n;i++)
    {
       
        if(a[i]!=b[i])
        {
            c++;
        }
    }
    cout<<c;
    return 0;
}